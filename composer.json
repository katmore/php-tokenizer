{
	"name": "katmore/php-tokenizer",
	"description": "composer.json",
	"homepage": "https://github.com/katmore/php-tokenizer",
	"license": [
		"MIT",
		"GPL-3.0+"
	],
	"require": {
		"php": ">=7.2.1",
		"psr/http-message": "^1.0",
		"fig/event-dispatcher-util": "^1.1",
		"phpdocumentor/reflection-docblock": "^4.3"
	},
	"autoload": {
		"psr-4": {
			"Katmore\\Tokenizer\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Katmore\\Tokenizer\\TestCase\\": "tests/phpunit/TestCase",
			"Katmore\\Tokenizer\\Tests\\": "tests/phpunit/Tests"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"phpunit/php-code-coverage": "^6.0",
		"php-di/phpdoc-reader": "^2.1"
	}
}
